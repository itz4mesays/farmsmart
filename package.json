{
  "name": "farmsmart",
  "version": "1.0.0",
  "description": "APIs developed for Farm Smart Application",
  "main": "server.js",
  "scripts": {
    "dev": "env NODE_ENV=development nodemon src/index.js",
    "migrate": "npx sequelize-cli db:migrate --url mysql://root:@127.0.0.1/farmsmart",
    "migrate_test": "npx sequelize-cli db:migrate --url mysql://$DB_USER:$DB_PASS@127.0.0.1/$DB_NAME",
    "migration": "npx sequelize-cli migration:create --name modify_users_add_new_fields",
    "model_generate": "npx sequelize-cli model:generate --name ModelName --attributes userid:integer,field2:string,field3:date",
    "seeder": "npx sequelize-cli seed:create --name my-seed-file",
    "seed_local": "npx sequelize-cli db:seed:all --url mysql://root:@127.0.0.1/farmsmart",
    "seed-single": "npx sequelize-cli db:seed --seed <name of seed file>",
    "test": "NODE=test yarn jest --silent=true --verbose --forceExit --runInBand",
    "test_local": "NODE=test yarn jest --silent=false --verbose --forceExit --runInBand",
    "test2": "NODE=test jest --forceExit --runInBand"
  },
  "author": "Oyedele Olufemi",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "deep-email-validator": "^0.1.18",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.2.0",
    "express-validator": "^6.11.1",
    "formidable": "^1.2.2",
    "g": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.0.2",
    "nodemailer": "^6.5.0",
    "request": "^2.88.2",
    "sequelize": "^5.21.7",
    "sequelize-cli": "^6.2.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "sweetalert": "^2.1.2",
    "sweetalert2": "^10.14.0",
    "unique-string": "^2.0.0"
  },
  "devDependencies": {
    "esm": "^3.2.25",
    "faker": "^5.5.3",
    "helmet": "^4.6.0",
    "jest": "^27.0.1",
    "jest-cli": "^27.0.4",
    "morgan": "^1.10.0",
    "supertest": "^6.1.3"
  }
}
